    "core-js": "3",
    "css-loader": "^3.4.2",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "qs": "^6.9.1",
    "raven-js": "^3.10.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-ga": "^2.7.0",
